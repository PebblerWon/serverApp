3"}]},{"type":"text","text":"):"}]},{"type":"text","text":"\n"},{"type":"node","name":"div","attrs":{"class":"highlight highlight-source-js"},"children":[null]},{"type":"text","text":"\n"},{"type":"node","name":"p","attrs":{},"children":[{"type":"text","text":"In Node:"}]},{"type":"text","text":"\n"},{"type":"node","name":"div","attrs":{"class":"highlight highlight-source-js"},"children":[null]},{"type":"text","text":"\n"},{"type":"node","name":"p","attrs":{},"children":[{"type":"text","text":"You can also require individual modules and combine them into a "},{"type":"node","name":"code","attrs":{},"children":[{"type":"text","text":"d3"}]},{"type":"text","text":" object using "},{"type":"node","name":"a","attrs":{"href":"https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Object/assign","rel":"nofollow"},"children":[{"type":"text","text":"Object.assign"}]},{"type":"text","text":":"}]},{"type":"text","text":"\n"},{"type":"node","name":"div","attrs":{"class":"highlight highlight-source-js"},"children":[null]},{"type":"text","text":"\n"}]